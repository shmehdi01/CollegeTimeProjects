<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="54.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FeeManagement.component.FooterController">
